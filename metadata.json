{
  "name": "puppetlabs-openstack",
  "version": "3.0.0",
  "author": "Puppet Labs and Community Contributors",
  "summary": "Puppet Labs Openstack Module targeted for Havana.",
  "license": "Apache License, Version 2.0",
  "source": "https://github.com/guessi/puppet-openstack.git",
  "project_page": "https://github.com/guessi/puppet-openstack",
  "issues_url": "https://github.com/guessi/puppet-openstack",
  "requirements": [
    { "name": "pe","version_requirement": "3.x" },
    { "name": "puppet","version_requirement": "3.x" }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": ["12.04"]
    }
  ],
  "description": "Puppet module that pulls together all the individual components of Openstack, resulting is a complete and functional stack.",
  "dependencies": [
    { "name": "puppetlabs/keystone", 	"version_requirement": ">=3.2.0 <4.0.0" },
    { "name": "puppetlabs/swift", 	"version_requirement": ">=3.0.0 <4.0.0" },
    { "name": "puppetlabs/cinder", 	"version_requirement": ">=3.1.1 <4.0.0" },
    { "name": "puppetlabs/glance", 	"version_requirement": ">=3.1.0 <4.0.0" },
    { "name": "puppetlabs/neutron", 	"version_requirement": ">=3.3.0 <4.0.0" },
    { "name": "puppetlabs/nova", 	"version_requirement": ">=3.2.1 <4.0.0" },
    { "name": "puppetlabs/heat", 	"version_requirement": ">=3.1.0 <4.0.0" },
    { "name": "puppetlabs/ceilometer", 	"version_requirement": ">=3.1.1 <4.0.0" },
    { "name": "puppetlabs/horizon", 	"version_requirement": ">=3.1.0 <4.0.0" },

    { "name": "camptocamp/kmod",        "version_requirement": ">=2.1.0 <3.0.0" },

    { "name": "puppetlabs/ntp", 	"version_requirement": ">=3.3.0 <4.0.0" },
    { "name": "puppetlabs/firewall", 	"version_requirement": ">=1.7.1 <2.0.0" },
    { "name": "puppetlabs/vswitch", 	"version_requirement": ">=0.3.0 <1.0.0" },
    { "name": "puppetlabs/mongodb", 	"version_requirement": ">=0.11.0 <1.0.0" }
  ]
}
